<div class="max-w-xs mx-auto bg-white rounded-lg shadow-md overflow-hidden mt-24">
    <div class="bg-gray-100 px-4 py-2">
        <h2 class="text-lg font-medium text-gray-800">Profile <%= data.contact.name %></h2>
    </div>
    <div class="px-4 py-5 sm:p-6">
        <div class="flex flex-col items-start justify-between mb-6">
            <span class="text-sm font-medium text-gray-600">Name</span>
            <span class="text-lg font-medium text-gray-800"> <%= data.contact.name %></span>
        </div>
        <div class="flex flex-col items-start justify-between mb-6">
            <span class="text-sm font-medium text-gray-600">Email</span>
            <span class="text-lg font-medium text-gray-800"> <%= data.contact.email %></span>
        </div>
        <div class="flex flex-row items-center justify-between mb-6">
            <div class="flex flex-col items-start">
                <span class="text-sm font-medium text-gray-600">Mobile Phone</span>
                <span class="text-lg font-medium text-gray-800"> <%= data.contact.phone %></span>
            </div>
           
        </div>
       
            
        </div>
    </div>
</div>